{
    "manifest_version": 2,
    
    "name": "Ingress Map Screener",
    "description": "Пока сам не скринит, только готовит карту для скрина",
    "version": "1.0",
    
    "permissions": [
        "https://www.ingress.com/intel",
        "tabs",
        "activeTab",
        "downloads"
    ],
    
    "background": {
        "scripts": ["background.js"]
    },
    
    "page_action": {  
        "default_icon": "icon.png",
        "default_popup": "popup.html"   
    }
}
